<a href="exampleProfileDev">

  <div class="card">
    
    <h2>Name</h2>
    
    <p>{{user.username}}</p>
    
    <h2>Email</h2>
    
    <p>{{user.email}}</p>
    
    <h2>Description</h2>
    
    <div class="description">
      <p>{{user.description}}</p>
    </div>

    {{#if user.userHasLiked}}
      <div class="like-btn like-btn-red" user-id="{{user._id}}" user-status="user"></div>
    {{else}}
      <div class="like-btn like-btn-bw" user-id="{{user._id}}" user-status="user"></div>
    {{/if}}

  </div>

</a>

<br>
